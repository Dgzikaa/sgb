{
  "name": "zykor-frontend",
  "version": "2.0.0",
  "description": "Zykor - O núcleo da gestão de bares - Frontend React",
  "private": true,
  "scripts": {
    "dev": "next dev --port 3001",
    "build": "next build",
    "build:analyze": "NEXT_PUBLIC_ANALYZE_BUNDLE=true next build",
    "build:production": "NODE_ENV=production next build && next-sitemap",
    "start": "next start",
    "start:production": "NODE_ENV=production next start -p 3000",
    "type-check": "tsc --noEmit",
    "type-check:strict": "tsc --noEmit --strict",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "lint:production": "next lint --max-warnings=0 --quiet",
    "code-quality": "npm run format:check && npm run lint:production",
    "build:check": "next build --no-lint",
    "monitoring:health": "curl -f http://localhost:3001/api/health || exit 1",
    "monitoring:test": "node scripts/test-monitoring.js",
    "sentry:check": "sentry-cli --version",
    "clean": "rm -rf .next out dist",
    "clean:cache": "rm -rf .next/cache",
    "precommit": "npm run code-quality && npm run type-check",
    "prebuild": "npm run clean && npm run type-check",
    "postbuild": "echo '✅ Build completed successfully'",
    "logs:on": "node scripts/toggle-logs.js on",
    "logs:off": "node scripts/toggle-logs.js off",
    "logs:status": "node scripts/toggle-logs.js status",
    "dev:quiet": "npm run logs:off && npm run dev",
    "dev:verbose": "npm run logs:on && npm run dev"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.60.0",
    "@dnd-kit/core": "^6.3.1",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/sdk-node": "^0.203.0",
    "@opentelemetry/semantic-conventions": "^1.36.0",
    "@radix-ui/react-collapsible": "^1.1.11",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-slider": "^1.3.6",
    "@radix-ui/react-tabs": "^1.0.4",
    "@radix-ui/react-toast": "^1.2.15",
    "@sentry/nextjs": "^10.5.0",
    "@supabase/supabase-js": "^2.38.4",
    "@types/qrcode": "^1.5.5",
    "@types/qrcode.react": "^3.0.0",
    "@types/react-big-calendar": "^1.16.2",
    "@vercel/blob": "^0.22.1",
    "@zxing/library": "^0.21.3",
    "archiver": "^7.0.1",
    "autoprefixer": "10.4.21",
    "axios": "^1.6.2",
    "bcryptjs": "^3.0.2",
    "chart.js": "^4.5.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "date-fns": "^2.30.0",
    "dotenv": "^17.2.1",
    "framer-motion": "^12.23.12",
    "google-auth-library": "^10.2.1",
    "googleapis": "^155.0.1",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.294.0",
    "luxon": "^3.7.1",
    "mercadopago": "^2.8.0",
    "next": "14.2.30",
    "openai": "^4.20.1",
    "passkit-generator": "^3.4.0",
    "postcss": "8.4.31",
    "qr-scanner": "^1.4.2",
    "qrcode": "^1.5.4",
    "qrcode.react": "^4.2.0",
    "react": "^18.2.0",
    "react-big-calendar": "^1.19.4",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "^18.2.0",
    "recharts": "^2.8.0",
    "resend": "^6.0.1",
    "tailwind-merge": "^2.0.0",
    "tailwindcss": "3.4.17",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@axe-core/react": "^4.8.2",
    "@types/bcryptjs": "^3.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20.10.0",
    "@types/react": "^18.2.39",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "critters": "^0.0.25",
    "eslint": "^8.54.0",
    "eslint-config-next": "14.0.4",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "prettier": "^3.1.0",
    "typescript": "^5.3.2"
