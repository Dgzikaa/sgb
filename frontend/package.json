{
  "name": "zykor-frontend",
  "version": "2.0.0",
  "description": "Zykor - O núcleo da gestão de bares - Frontend React",
  "private": true,
  "scripts": {
    "dev": "next dev --port 3001",
    "build": "next build",
    "build:analyze": "NEXT_PUBLIC_ANALYZE_BUNDLE=true next build",
    "build:production": "NODE_ENV=production next build && next-sitemap",
    "start": "next start",
    "start:production": "NODE_ENV=production next start -p 3000",
    "type-check": "tsc --noEmit",
    "type-check:strict": "tsc --noEmit --strict",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "lint:production": "next lint --max-warnings=0 --quiet",
    "code-quality": "npm run format:check && npm run lint:production",
    "build:check": "next build --no-lint",
    "monitoring:health": "curl -f http://localhost:3001/api/health || exit 1",
    "monitoring:test": "node scripts/test-monitoring.js",
    "sentry:check": "sentry-cli --version",
    "clean": "if exist .next rmdir /s /q .next && if exist out rmdir /s /q out && if exist dist rmdir /s /q dist",
    "clean:cache": "if exist .next\\cache rmdir /s /q .next\\cache",
    "precommit": "npm run code-quality && npm run type-check",
    "prebuild": "npm run clean && npm run type-check",
    "postbuild": "echo '✅ Build completed successfully'",
    "logs:on": "node scripts/toggle-logs.js on",
    "logs:off": "node scripts/toggle-logs.js off",
    "logs:status": "node scripts/toggle-logs.js status",
    "dev:quiet": "npm run logs:off && npm run dev",
    "dev:verbose": "npm run logs:on && npm run dev"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.62.0",
    "@dnd-kit/core": "^6.3.1",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/sdk-node": "^0.205.0",
    "@opentelemetry/semantic-conventions": "^1.37.0",
    "@radix-ui/react-collapsible": "^1.1.12",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slider": "^1.3.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-toast": "^1.2.15",
    "@sentry/nextjs": "^10.11.0",
    "@supabase/supabase-js": "^2.57.4",
    "@types/qrcode": "^1.5.5",
    "@types/react-big-calendar": "^1.16.2",
    "@vercel/blob": "^1.1.1",
    "@zxing/library": "^0.21.3",
    "archiver": "^7.0.1",
    "autoprefixer": "10.4.21",
    "axios": "^1.11.0",
    "bcryptjs": "^3.0.2",
    "chart.js": "^4.5.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.2",
    "framer-motion": "^12.23.12",
    "google-auth-library": "^10.3.0",
    "googleapis": "^159.0.0",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.543.0",
    "luxon": "^3.7.2",
    "mercadopago": "^2.9.0",
    "next": "^15.5.3",
    "openai": "^5.20.1",
    "passkit-generator": "^3.4.0",
    "postcss": "8.5.6",
    "qr-scanner": "^1.4.2",
    "qrcode": "^1.5.4",
    "qrcode.react": "^4.2.0",
    "react": "^19.1.1",
    "react-big-calendar": "^1.19.4",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "^19.1.1",
    "recharts": "^3.2.0",
    "resend": "^6.0.3",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^3.4.15",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@axe-core/react": "^4.10.2",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^24.3.1",
    "@types/react": "^19.1.12",
    "@types/react-dom": "^19.1.9",
    "@typescript-eslint/eslint-plugin": "^8.43.0",
    "@typescript-eslint/parser": "^8.43.0",
    "eslint": "^9.35.0",
    "eslint-config-next": "15.5.3",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "prettier": "^3.6.2",
    "typescript": "^5.9.2"
  },
  "keywords": [
    "zykor",
    "bar",
    "gestão",
    "contahub",
    "sympla",
    "yuzer",
    "react",
    "supabase",
    "nextjs",
    "pwa",
    "enterprise"
  ],
  "repository": {
    "type": "git",
    "url": "local"
  },
  "author": "Zykor Team",
  "license": "MIT"
}
