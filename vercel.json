{
  "buildCommand": "cd frontend && npm run build",
  "outputDirectory": "frontend/.next",
  "installCommand": "cd frontend && npm install",
  "devCommand": "cd frontend && npm run dev",
  "crons": [
    {
      "path": "/api/configuracoes/credenciais/nibo-sync-simple",
      "schedule": "0 */6 * * *"
    },
    {
      "path": "/api/configuracoes/desempenho/automacao-semanal",
      "schedule": "0 9 * * 1"
    }
  ],
  "functions": {
    "frontend/src/app/api/configuracoes/credenciais/nibo-sync/route.ts": {
      "maxDuration": 300
    },
    "frontend/src/app/api/configuracoes/desempenho/automacao-semanal/route.ts": {
      "maxDuration": 300
    }
  },
  "git": {
    "deploymentEnabled": {
      "main": true
    }
  },
  "github": {
    "autoAlias": false
  }
}
